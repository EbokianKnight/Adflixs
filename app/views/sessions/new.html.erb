<%= render "shared/form" %>
<%= render "shared/errors" %>

<div>
	<h1>Sign In</h1>
	<form class="sign-in-form" action="<%= session_url %>" method="post">
		<%= auth_token %>
		<label>Email
			<input type="text" name="user[email]" value="">
		</label>
		<label>Password
			<input type="password" name="user[password]" value="">
		</label>
		<input type="submit" value="Sign Up">
	</form>
</div>
